<div class="evaluation-request" data-workspace-entity-id="{workspaceEntityId}">
  <!-- CLASS:
  evaluation-requested: arviointi pyydetty 
  evaluated-incomplete: täydennettävä 
  evaluated-passed: arvioitu 
  -->
  <div class="evaluation-request-title">
    <div class="evaluation-request-student">{lastName}, {firstName}</div>
    <div class="evaluation-request-study-programme">{studyProgramme}</div>
  </div>
  <div class="evaluation-request-data">
    <div class="evaluation-request-data-row">
      <span class="evaluation-request-data-label">Ilmoittautunut kurssille</span>
      <span class="evaluation-request-data-text">{enrollmentDate|formatDate}</span>
    </div>
    <div class="evaluation-request-data-row highlight">
      <span class="evaluation-request-data-label">Arviointipyyntö jätetty</span>
      <span class="evaluation-request-data-text">{assessmentRequestDate|formatDate}</span>
    </div>
    <div class="evaluation-request-data-row">
      <span class="evaluation-request-data-label">Tehtäviä tehty</span>
      <span class="evaluation-request-data-text assignments-status {@eq key=assignmentsDone value=assignmentsTotal}all-done{/eq} {@ne key=assignmentsDone value=assignmentsTotal}not-done{/ne}">{assignmentsDone}/{assignmentsTotal}</span>
    </div>
    <div class="evaluation-request-button-row">
      <div class="evaluation-request-card-button delete-button icon-delete" title="Poista opiskelijan opintojakson arviointi"></div>
      <div class="evaluation-request-card-button evaluate-button icon-evaluate" title="Arvioi Opiskelijan opintojakso"></div>
    </div>
  </div>
</div>