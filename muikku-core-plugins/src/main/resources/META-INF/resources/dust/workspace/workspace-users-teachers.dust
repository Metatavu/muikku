      <div class="workspace-users-name">{lastName}, {firstName}</div>
      <!--
      <div class="workspace-users-delete">{#localize key="plugin.workspace.users.student.deleteLink.label"/}</div>
      -->
    </div>
  {:else}
    <div class="workspace-users-empty">
      {#localize key="plugin.workspaces.users.teachers.empty"/}
    </div>
  {/teachers}
  
</div>