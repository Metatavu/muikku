{?educationTypes}
  <div class="mf-categories">
    <div class="mf-category-title"><span>{#localize key="plugin.coursepicker.filters.title"/}</span></div>
    <ul class="mf-category-content cp-filters-ul"> 
      {#educationTypes}
        <li id="{type}-{id}">
          <a class="mf-category-item-plain cp-education-type-filter" data-id="{identifier}">
            <span>{name}</span>
          </a></li>    
        </li>    
      {/educationTypes}
    </ul>
  </div>
{/educationTypes}

