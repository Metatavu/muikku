<section class="evaluation-modal-studentAssignmentWrapper" id="evaluationStudentAssignmentWrapper">
  <article class="evaluation-modal-student-wrapper">
    <div class="evaluation-modal-workspace-assignment-info">
      <div class="evaluation-modal-workspace-assignment-wrapper">
        {#assignments}
          {>"evaluation/evaluation_evaluate_workspace_single_assigment.dust"/}
        {/assignments}
      </div>
    </div>
    <div class="evaluation-modal-workspace-journal-info">
      <div class="evaluation-modal-workspace-journal-entries-wrapper">
        <div class="evaluation-modal-workspace-journal-entries-title">{#localize key="plugin.evaluation.workspaceEvaluationDialog.jhournalEntries.title"/}</div>
        {#journalEntries}
          {>"evaluation/evaluation_evaluate_workspace_single_journal_entry.dust"/}
        {/journalEntries}
      </div>
    </div>
  </article>
</section>