{
  "default": {
    "cacheStrategy": "NONE",
    "maxEntries": 0,
    "expireTime": 0,
    "enabledCaches": []
  },
  "settings": {
    "/students/students/{studentId}": {
      "cacheStrategy": "EXPIRES",
      "maxEntries": 100,
      "expireTime": 1800000,
      "enabledCaches": ["USER", "SYSTEM"],
      "evictOn": ["STUDENT_UPDATE", "STUDENT_ARCHIVE"]
    },
    "/persons/persons/{personId}": {
      "cacheStrategy": "EXPIRES",
      "maxEntries": 100,
      "expireTime": 1800000,
      "enabledCaches": ["USER", "SYSTEM"],
      "evictOn": ["PERSON_UPDATE", "PERSON_ARCHIVE"]
    },
    "/staff/members/{staffMemberId}": {
      "cacheStrategy": "EXPIRES",
      "maxEntries": 100,
      "expireTime": 1800000,
      "enabledCaches": ["USER", "SYSTEM"],
      "evictOn": ["STAFF_MEMBER_UPDATE", "STAFF_MEMBER_DELETE"] 
    }
  }
  
}